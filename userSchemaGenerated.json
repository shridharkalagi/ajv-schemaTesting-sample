{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "foo",
      "bar"
    ],
    "properties": {
      "foo": {
        "$id": "#/properties/foo",
        "type": "string",
        "title": "The Foo Schema",
        "default": "",
        "examples": [
          "abc"
        ],
        "pattern": "^(.*)$"
      },
      "bar": {
        "$id": "#/properties/bar",
        "type": "integer",
        "title": "The Bar Schema",
        "default": 0,
        "examples": [
          3
        ],
        "maximum": 3 
      }
    }
  }